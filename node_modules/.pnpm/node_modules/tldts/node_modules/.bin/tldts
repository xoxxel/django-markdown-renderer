#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/behroz/OneDrive/Desktop/input/node_modules/.pnpm/tldts@7.0.16/node_modules/tldts/bin/node_modules:/mnt/c/Users/behroz/OneDrive/Desktop/input/node_modules/.pnpm/tldts@7.0.16/node_modules/tldts/node_modules:/mnt/c/Users/behroz/OneDrive/Desktop/input/node_modules/.pnpm/tldts@7.0.16/node_modules:/mnt/c/Users/behroz/OneDrive/Desktop/input/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/behroz/OneDrive/Desktop/input/node_modules/.pnpm/tldts@7.0.16/node_modules/tldts/bin/node_modules:/mnt/c/Users/behroz/OneDrive/Desktop/input/node_modules/.pnpm/tldts@7.0.16/node_modules/tldts/node_modules:/mnt/c/Users/behroz/OneDrive/Desktop/input/node_modules/.pnpm/tldts@7.0.16/node_modules:/mnt/c/Users/behroz/OneDrive/Desktop/input/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
